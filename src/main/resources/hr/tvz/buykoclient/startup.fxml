<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="1080.0" prefWidth="1920.0" spacing="20.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="hr.tvz.buykoclient.controllers.StartupController">
   <AnchorPane prefHeight="1080.0" prefWidth="1920.0">
      <children>
      
          <Label fx:id="welcomeText" layoutX="794.0" layoutY="106.0" text="Welcome to Buyko!">
            <font>
               <Font size="30.0" />
            </font>
         </Label>
          <Button fx:id="registerButton" layoutX="858.0" layoutY="449.0" onAction="#onRegisterButtonClick" text="Register">
            <font>
               <Font size="30.0" />
            </font>
         </Button>
         <Button fx:id="loginButton" layoutX="875.0" layoutY="366.0" mnemonicParsing="false" onAction="#onLogInButtonClick" text="Log in">
            <font>
               <Font size="30.0" />
            </font>
         </Button>
      </children>
   </AnchorPane>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
